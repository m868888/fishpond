{
    "name": "m868888/fishpond",
    "autoload": {
        "psr-4": {
            "Fishpond\\Fishpond\\": "packages/fishpond/src"
        }
    },
    "replace": {
        "fishpond/fishpond": "self.version"
    },
    "description": "The project is a framework for integration game platform.",
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0",
        "ramsey/uuid": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symplify/monorepo-builder": "^8.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
